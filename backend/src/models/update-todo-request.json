{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todos",
  "type": "object",
  "properties": {
    "todoId": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "dueDate": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },
    "done": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "string"
    }
  },
  "required": ["todoId", "createdAt", "name", "dueDate", "userId", "done", "attachmentUrl"],
  "additionalProperties": false
}
